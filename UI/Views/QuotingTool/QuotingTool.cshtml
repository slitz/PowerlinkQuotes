                            @Html.HiddenFor(m => m.NavCategory, new { @Value = "kits" })
                            @Html.HiddenFor(m => m.SelectedProductsList)
                            @Html.ListBoxFor(m => m.SelectedKitsList, Model.KitDataList, new Dictionary<string, object>() {{"class", "form-control"}, 
